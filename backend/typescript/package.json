{
  "name": "avila-protocol-market-data",
  "version": "1.0.0",
  "description": "Comprehensive market data service for Avila Protocol - fetches from 10+ free APIs with fallback logic",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node --watch server.ts",
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "postinstall": "npm run build",
    "test": "ts-node test_marketData.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "market-data",
    "tradfi",
    "defi",
    "crypto",
    "options-trading",
    "aptos",
    "blockchain",
    "api-integration",
    "real-time-data",
    "fallback-logic"
  ],
  "author": "Avila Protocol Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
