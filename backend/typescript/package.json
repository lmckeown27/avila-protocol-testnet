{
  "name": "@avila-protocol/backend",
  "version": "1.0.0",
  "description": "Backend TypeScript services for Avila Protocol",
  "main": "deploy.js",
  "scripts": {
    "build": "tsc",
    "test": "cd ../smart-contracts && aptos move test",
    "deploy:testnet": "node deploy.js full",
    "deploy:mainnet": "NODE_URL=https://fullnode.mainnet.aptoslabs.com node deploy.js full",
    "initialize": "node deploy.js initialize",
    "verify": "node deploy.js verify",
    "lint": "cd ../smart-contracts && aptos move check",
    "clean": "cd ../smart-contracts && aptos move clean",
    "format": "cd ../smart-contracts && aptos move format"
  },
  "keywords": [
    "aptos",
    "move",
    "options",
    "defi",
    "trading",
    "blockchain",
    "typescript",
    "backend"
  ],
  "author": "Avila Protocol Team",
  "license": "MIT",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^4.0.0",
    "aptos": "^1.21.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.122",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
